        <div class="clearfix mar-15">
            <div class="col-xs-24 col-sm-24 col-md-12">
                [SOCIAL_ICONS]
            </div>
            <div class="col-xs-24 col-sm-24 col-md-12">
                [COPYRING]
            </div>
        </div>
    </div>
